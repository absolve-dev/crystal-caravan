<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h3>Orders:</h3>
      <%= "You have no orders." if @orders.respond_to?(:length) && @orders.length == 0 %>
      <% for order in @orders %>
        <%= link_to("##{order.id}", order_path(order)) %>
      <% end %>
        <br><br><%= link_to("Change E-mail/Password or Delete Account", edit_user_registration_path) %>
    </div>
  </div>
</div>