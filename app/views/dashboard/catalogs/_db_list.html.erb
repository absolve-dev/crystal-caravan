<div class="col-md-6">
  <h3>Database Sets</h3>
  <% if @db_sets.length > 0 %>
    <ul class="list-group">
      <% for set in @db_sets do %>
        <li class="list-group-item">
          <span class="badge"><%= set.catalog_cards.count %></span>
          <%= link_to set.name, dashboard_catalog_set_path(set) %>
        </li>
      <% end %>
    </ul>
  <% else %>
    Database did not return any sets
  <% end %>
</div>