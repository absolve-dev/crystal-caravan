<p id="notice"><%= notice %></p>

<h1>Listing Catalogs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalogs.each do |catalog| %>
      <tr>
        <td><%= link_to 'Show', dashboard_catalog_path(catalog) %></td>
        <td><%= link_to 'Edit', edit_dashboard_catalog_path(catalog) %></td>
        <td><%= link_to 'Destroy', dashboard_catalog_path(catalog), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Catalog', new_dashboard_catalog_path %>
